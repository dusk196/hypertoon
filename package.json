{
  "name": "hypertoon",
  "version": "1.2.0",
  "description": "High-performance, type-safe TOON (Token-Oriented Object Notation) converter. Faster than light.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunup ./src/index.ts --minify",
    "test": "bun test",
    "prepublishOnly": "bun run build",
    "benchmark": "bun run benchmark.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dusk196/hypertoon"
  },
  "keywords": [
    "json",
    "toon",
    "converter",
    "parser",
    "serializer",
    "tabular",
    "compression",
    "optimization",
    "bun",
    "typescript",
    "hypertoon"
  ],
  "author": {
    "name": "Sayantan Roy",
    "email": "sayantan.roy94@gmail.com",
    "url": "https://lazyguy.in/"
  },
  "license": "MIT",
  "devDependencies": {
    "@toon-format/toon": "^2.1.0",
    "bun-types": "latest",
    "bunup": "latest",
    "json-toon": "^1.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}